<!DOCTYPE html>
<html>
<head>
  <title>List</title>
</head>
<body>
  <button onclick="location.href='/push'">생성하기</button>  
  <button onclick="location.href='/login'">로그인</button><button onclick="location.href='/join'">회원가입</button>
  <button onclick="location.href='/logout'">로그아웃</button>
  <% if (lists && lists.length) { %>
    <% lists.forEach(list => { %>
      <p>&nbsp;&nbsp;제목  : &nbsp;<%= list.title %>&nbsp;&nbsp;내용: &nbsp;<%= list.content %></p>
      <button onclick="location.href='/modify/<%=list.id%>'">수정</button><button onclick="location.href='/delete/<%=list.id%>'">삭제</button>
    <% }); %>
  <% } else { %>
    <p>No lists found.</p>
  <% } %>
</body>
</html>